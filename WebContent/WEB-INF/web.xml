<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <servlet>
    <servlet-name>CalServlet</servlet-name>
    <servlet-class>controller.CalServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CalServlet</servlet-name>
    <url-pattern>/CalProcessing</url-pattern>
  </servlet-mapping>
  
  <resource-ref>
       <description>Oralce11g Datasource example</description>
       <res-ref-name>jdbc/Oracle11g</res-ref-name>
       <res-type>javax.sql.DataSource</res-type>
       <res-auth>Container</res-auth>
     </resource-ref>
</web-app>